<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="window" prefHeight="173.0" prefWidth="251.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controle.FormBaiaController">
   <children>
      <VBox layoutX="1.0" layoutY="-1.0" prefHeight="104.0" prefWidth="250.0" spacing="20.0">
         <children>
            <TextField fx:id="tamanho" promptText="Tamanho" styleClass="input" />
            <CheckBox fx:id="limpo" mnemonicParsing="false" text="Limpo" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER_RIGHT" layoutX="1.0" layoutY="103.0" prefHeight="78.0" prefWidth="250.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onCancel" text="Cancelar">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-danger" />
               </styleClass></Button>
            <Button mnemonicParsing="false" onAction="#onSaveBaia" text="Salvar">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-success" />
               </styleClass></Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
